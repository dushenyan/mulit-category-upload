{
  "name": "mulit-category-upload",
  "description": "多分类上传组件",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel serve dev:client",
    "dev:client": "vite",
    "serve": "tsx watch ./server/index.ts",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "cors": "^2.8.5",
    "element-plus": "^2.11.4",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "spark-md5": "^3.0.2",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.0",
    "@types/spark-md5": "^3.0.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.3",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vue-tsc": "^3.1.1"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    },
    "patchedDependencies": {
      "rolldown-vite@7.1.14": "patches/rolldown-vite@7.1.14.patch"
    }
  }
}
